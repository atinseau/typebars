{
	"name": "typebars",
	"version": "1.0.4",
	"license": "MIT",
	"description": "Typebars is a type-safe handlebars based template engine for generating object or content with static type checking",
	"author": {
		"email": "arthurtinseau@live.fr",
		"url": "https://github.com/atinseau/typebars",
		"name": "atinseau"
	},
	"type": "module",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/esm/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/esm/index.d.ts",
				"default": "./dist/esm/index.js"
			},
			"require": {
				"types": "./dist/cjs/index.d.ts",
				"default": "./dist/cjs/index.js"
			}
		}
	},
	"sideEffects": false,
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"prepare": "lefthook install",
		"clean": "rm -rf dist",
		"build:esm": "swc src -d dist/esm --strip-leading-paths --config-file .swcrc.esm.json",
		"build:cjs": "swc src -d dist/cjs --strip-leading-paths --config-file .swcrc.cjs.json",
		"build:types:esm": "tsc --project tsconfig.build.json",
		"build:types:cjs": "tsc --project tsconfig.build.cjs.json",
		"build:swc": "bun --parallel build:esm build:cjs",
		"build:types": "bun --parallel build:types:esm build:types:cjs",
		"build": "bun run clean && bun --parallel build:swc build:types && bun run postbuild",
		"postbuild": "bun scripts/postbuild.ts",
		"test:integration": "bun test tests/integration/",
		"prepublishOnly": "bun run build",
		"check-types": "tsc --noEmit",
		"check": "biome check --write --unsafe",
		"test": "bun test"
	},
	"devDependencies": {
		"@biomejs/biome": "2.4.3",
		"@swc/cli": "0.8.0",
		"@swc/core": "1.15.17",
		"@types/bun": "latest",
		"@types/json-schema": "7.0.15",
		"es-module-lexer": "^2.0.0",
		"lefthook": "2.1.1"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"handlebars": "4.7.8",
		"json-schema-to-ts": "3.1.1"
	}
}
